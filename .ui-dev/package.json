{
  "name": "egpt-ui",
  "version": "0.7.0",
  "private": true,
  "engines": {
    "node": ">=18",
    "npm": ">=9"
  },
  "dependencies": {
    "@egomobile/http-server": "0.63.1",
    "@egomobile/nodelike-utils": "0.4.3",
    "@egomobile/types": "2.0.0",
    "@tabler/icons-react": "2.29.0",
    "@types/lodash": "4.14.196",
    "@types/node": "18.11.9",
    "@types/react": "18.2.16",
    "@types/react-dom": "18.2.7",
    "@types/react-syntax-highlighter": "15.5.7",
    "@types/ua-parser-js": "0.7.36",
    "@types/uuid": "9.0.2",
    "autoprefixer": "10.4.14",
    "axios": "1.4.0",
    "clsx": "2.0.0",
    "cors": "2.8.5",
    "dayjs": "1.11.9",
    "del-cli": "5.0.0",
    "lodash": "4.17.21",
    "move-file-cli": "3.0.0",
    "postcss": "8.4.27",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hot-toast": "2.4.1",
    "react-markdown": "8.0.7",
    "react-scripts": "5.0.1",
    "react-syntax-highlighter": "15.5.0",
    "rehype-mathjax": "4.0.3",
    "remark-gfm": "3.0.1",
    "remark-math": "5.1.1",
    "tailwindcss": "3.3.3",
    "tsx": "3.12.7",
    "typescript": "4.9.5",
    "ua-parser-js": "1.0.35",
    "uuid": "9.0.0"
  },
  "scripts": {
    "build": "react-scripts build && npm run build:move && npm run gitkeep:create",
    "build:move": "del ../ui/build --force && move-file ./build ../ui/build",
    "gitkeep:create": "node -e \"const fs = require('node:fs'); const path = require('node:path'); const gitkeep = path.join(process.cwd(), '../ui/build/.gitkeep'); fs.writeFileSync(gitkeep, '', 'utf8');\"",
    "start": "tsx ./server.ts & PORT=8080 react-scripts start",
    "test": "react-scripts test"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}